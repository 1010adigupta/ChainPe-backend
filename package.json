    "@types/readable-stream": "^2.3.14",
    "aws-sdk": "^2.1201.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "concurrently": "^7.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "express": "~4.16.1",
    "express-http-proxy": "^1.6.3",
    "express-static-gzip": "^2.1.7",
    "hardhat": "^2.12.3",
    "helmet": "^5.1.0",
    "https": "^1.0.0",
    "mongodb": "^4.7.0",
    "mongoose": "^6.3.8",
    "mongoose-paginate-v2": "^1.6.3",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.1",
    "node-fetch": "^3.2.6",
    "nodemon": "^2.0.19",
    "react-player": "^2.10.1",
    "react-telegram-login": "^1.1.2",
