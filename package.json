{
  "name": "chainpebackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./bin/www",
    "start": "DEBUG=chainpebackend:* & nodemon ./bin/www",
    "client": "cd ./client && yarn run start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "build": "npm install --production && cd ./client && rm -rf node_modules"
  },
